@model IEnumerable<AutEmplAcc.Model.Employee>

<table class="table">
    <thead>
        <tr>
            <th>Имя</th>
            <th>Возраст</th>
            <th>Должность</th>
            <th>Адрес</th>
            <th>Паспортные данные</th>
            <th>Телефон</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.Name</td>
                <td>@employee.Age</td>
                <td>@employee.Position</td>
                <td>@employee.Address</td>
                <td>@employee.PassportData</td>
                <td>@employee.Phone</td>
            </tr>
        }
    </tbody>
</table>